#Excercise:
#  [4,8,9]    [3,8,12]   [0,0,0]
#  [2,3,5] +  [4,5,6 ] = [0,0,0]
#  [6,7,2]    [7,8,9 ]   [0,0,0]

A = [[4,8,9],
     [2,3,5],
     [6,7,2]]

B = [[3,8,12],
     [4,5,6],
     [7,8,9]]

C = [(A[0][0]+B[0][0],A[0][1]+B[0][1],A[0][2]+B[0][2]),
     (A[1][0]+B[1][0],A[1][1]+B[1][1],A[1][2]+B[1][2]),
     (A[2][0]+B[2][0],A[2][1]+B[2][1],A[2][2]+B[2][2])]

print(C)
